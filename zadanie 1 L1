zmienna1 = input("Podaj 1 liczbę: ")
zmienna2 = input("Podaj 2 liczbę: ")
suma = int(zmienna1) + int(zmienna2)
różnica = int(zmienna1) - int(zmienna2)
iloczyn = int(zmienna1) * int(zmienna2)
iloraz = int(zmienna1) / int(zmienna2)
print (("suma to:"), suma)
print (("różnica to:"), różnica)
print (("iloczyn to:"), iloczyn)
print (("iloraz to:"), iloraz)
